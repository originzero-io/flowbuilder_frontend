{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks",
        "jsx-a11y",
        "only-warn"
    ],
    
    "rules": {
        "react/display-name":"off",
        "class-methods-use-this": "warn",
        "eqeqeq":"warn",
        "no-case-declarations":"warn",
        "no-empty-pattern":"warn",
        "no-new-wrappers":"warn",
        "quotes": "off",
        "linebreak-style": "off",
        "react/function-component-definition": "off",
        "import/no-unresolved": "off"
    }
}
